<%= javascript_pack_tag "calendar" %>

<h1>Home page</h1>

<%= link_to "User sign in", new_user_session_path %>
<br>
<%= link_to "Admin sign in", new_administrator_session_path %>

<% if current_administrator %>
  <p><%= current_administrator.email %></p>
<% end %>

<% if current_user %>
  <p><%= current_user.email %></p>
<% end %>

<div class="calendar-container">
  <%= Calendar.new.show_calendar %>
</div>
